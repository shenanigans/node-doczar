<!DOCTYPE html>
<html>
<head>
    <title>{{pathstr}} documentation</title>
    <link rel="stylesheet" type="text/css" href="{{baseTagPath}}index.css" />
    <link rel="stylesheet" type="text/css" href="{{baseTagPath}}highlight.css" />
    <script type="text/javascript" src="{{baseTagPath}}index.js"></script>
</head>
<body class="{{ctype}}">
    <div id="header">
        <span class="ctype">{{ctype}}</span>
        {{#valtype}}
            <span class="valsep">|</span>
            <a class="valtype" href="{{link path}}">{{name}}</a>
        {{/valtype}}
        <span class="breadcrumbs">
            {{#breadcrumbs}}
                <span class="delimiter">{{delimiter}}</span>
                <a href="{{link path}}" class="breadcrumb {{ctype}}">{{name}}</a>
            {{/breadcrumbs}}
        </span>
    </div>
    <br />
    <div id="content">
        {{#if isFunction}}
            <h2 class="signature">
                {{name}} (
                    {{#arguments}}
                        {{> argument}}
                    {{/arguments}}
                ) {
            </h3>
        {{/if}}
        <!-- basic document info -->
        <div id="details" class="markdown">
            <div id="TOC">
                <h3>Table of Contents</h3>
                {{#if statics}}
                    <div class="section">
                        <h4 class="header"><a href="#statics">Static Properties</a></h4>
                        {{#statics}}
                            <a href="#property_{{name}}">{{name}}</a><br />
                        {{/statics}}
                    </div>
                {{/if}}
                {{#if functions}}
                    <div class="section">
                        <h4 class="header"><a href="#functions">Static Functions</a></h4>
                        {{#functions}}
                            <a href="#property_{{name}}">{{name}}</a><br />
                        {{/functions}}
                    </div>
                {{/if}}
                {{#if members}}
                    <div class="section">
                        <h4 class="header"><a href="#members">Instance Properties</a></h4>
                        {{#members}}
                            <a href="#member_{{name}}">{{name}}</a><br />
                        {{/members}}
                    </div>
                {{/if}}
                {{#if methods}}
                    <div class="section">
                        <h4 class="header"><a href="#methods">Instance Methods</a></h4>
                        {{#methods}}
                            <a href="#member_{{name}}">{{name}}</a><br />
                        {{/methods}}
                    </div>
                {{/if}}
            </div>
{{{markdown details}}}
            <br class="clear" />
        </div>
        {{#if spares}}
            <!-- extra document info -->
            <div id="spares" class="section">
                <h2>More Information</h2>
                {{#spares}}
                    <div class="spare">
                        <a href="spare/{{name}}/index.html">{{name}}</a>
                        <div class="markdown">{{{markdown doc}}}</div>
                    </div>
                {{/spares}}
            </div>
        {{/if}}
        {{#if hasConstructor}}
            <!-- constructor function -->
            <h2>Constructor</h2>
            <div id="constructor" class="function">
                <h3 class="signature">
                    {{name}} (
                        {{#arguments}}
                            {{> argument}}
                        {{/arguments}}
                    ) {
                </h3>
                <div class="markdown">{{{markdown doc}}}</div>
                <ul class="arguments">
                    {{#arguments}}
                        <li class="argument">
                            {{#valtype}}
                                <span class="valsep">|</span>
                                <a class="valtype" href="{{link path}}">{{name}}</a>
                            {{/valtype}}
                            <a class="title" href="argument/{{pathname}}/index.html">
                                {{name}}
                            </a>
                            <div class="markdown">{{{markdown summaryDoc}}}</div>
                        </li>
                    {{/arguments}}
                </ul>
            </div>
        {{/if}}
