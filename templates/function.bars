<div id="{{ctype}}_{{name}}" class="child function">
    <span class="signature">
        {{#flags}}
            <div class="flag {{.}}">{{.}}</div>
        {{/flags}}
        {{#returns}}
            <span class="valsep">,</span>
            {{#valtype}}
                <span>
                    <span class="valsep">|</span>
                    <a class="valtype" href="{{link path}}">{{name}}</a>
                </span>
            {{/valtype}}
        {{/returns}}
        <a href="{{simpleCtype}}/{{pathname}}/index.html" class="title">
            {{name}}
        </a>
        (
            {{#arguments}}
                {{> argument}}
            {{/arguments}}
        ) {
    </span>
    {{#if isInherited}}
        <div class="source super">
            Inherited from <a class="super" href="{{link source}}">{{sourcestr}}</a>
        </div>
    {{else}}
        {{#override}}
            <div class="source super">
                Overrides <a class="super" href="{{link path}}">{{pathstr}}</a>
            </div>
        {{/override}}
    {{/if}}
    {{#satisfies}}
        <div class="source interface">
            Satisfies interface <a class="interface" href="{{link path}}">{{pathstr}}</a>
        </div>
    {{/satisfies}}
    <div class="info">
        <span class="markdown">{{{markdown summaryDoc}}}</span>
        {{#if arguments}}
            <h4>Arguments</h4>
            <div class="section arguments">
                {{#arguments}}
                    <div class="child argument">
                        <span class="signature">
                            {{#flags}}
                                <div class="flag {{.}}">{{.}}</div>
                            {{/flags}}
                            <span>{{#valtype}}
                                <span class="valsep">|</span>
                                <a class="valtype" href="{{link path}}">{{name}}</a>
                            {{/valtype}}</span>
                            <a class="title" href="{{link path}}">
                                {{name}}
                            </a>
                        {{#if isCallback}}
                            (
                            {{#arguments}}
                                {{> argument}}
                            {{/arguments}}
                            ) {
                        {{/if}}
                        </span>
                        {{#if isInherited}}
                            <div class="source super">
                                Inherited from <a class="super" href="{{link source}}">{{sourcestr}}</a>
                            </div>
                        {{/if}}
                        <div class="info">
                            <span class="markdown">{{{markdown summaryDoc}}}</span>
                            {{#if isCallback}}
                                <h5>Arguments</h5>
                                <div class="section arguments">
                                    {{#arguments}}
                                        <div class="child {{ctype}}">
                                            <span class="signature">
                                                {{#if isInherited}}
                                                    <div class="source">
                                                        Inherited from <a class="super" href="{{link source}}">{{sourcestr}}</a>
                                                    </div>
                                                {{/if}}
                                                {{#if isKwarg}}
                                                    <a class="title" href="{{link path}}">
                                                        {{name}}
                                                    </a>
                                                    =
                                                    <span>{{#valtype}}
                                                        <span class="valsep">|</span>
                                                        <a class="valtype" href="{{link path}}">{{name}}</a>
                                                    {{/valtype}}</span>
                                                {{else}}
                                                    <span>{{#valtype}}
                                                        <span class="valsep">|</span>
                                                        <a class="valtype" href="{{link path}}">{{name}}</a>
                                                    {{/valtype}}</span>
                                                    <a class="title" href="{{link path}}">
                                                        {{name}}
                                                    </a>
                                                {{/if}}
                                            </span>
                                            <div class="info">
                                                <span class="markdown">{{{markdown summaryDoc}}}</span>
                                            </div>
                                        </div>
                                    {{/arguments}}
                                </div>
                            {{/if}}
                        </div>
                    </div>
                {{/arguments}}
            </div>
        {{/if}}
        {{#if returns}}
            <h4>returns</h4>
            <div class="section returns">
                {{#returns}}
                    <div class="return">
                        {{> child}}
                    </div>
                {{/returns}}
            </div>
        {{/if}}
        <div class="tail" /></div>
    </div>
</div>
