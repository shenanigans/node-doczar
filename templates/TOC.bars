<div id="TOC">
    {{#if isFunction}}
        {{#if arguments}}
            <div class="section">
                <h4 class="header"><a href="#arguments">Arguments</a></h4>
                {{#arguments}}
                    <a href="{{softlink path}}"{{#if isInherited}} class="inherited"{{/if}}>{{name}}</a><br />
                {{/arguments}}
            </div>
        {{/if}}
    {{/if}}
    {{#if modules}}
        <div class="section">
            <h4 class="header"><a href="#modules">Modules</a></h4>
            {{#modules}}
                <a href="{{softlink path}}">{{name}}</a><br />
            {{/modules}}
        </div>
    {{/if}}
    {{#if enums}}
        <div class="section">
            <h4 class="header"><a href="#enums">Enumerations</a></h4>
            {{#enums}}
                <a href="{{softlink path}}"{{#if isInherited}} class="inherited"{{/if}}>{{name}}</a><br />
            {{/enums}}
        </div>
    {{/if}}
