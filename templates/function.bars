<div id="{{elemID}}" class="child function">
    <span class="signature">
        {{#flags}}
            <div class="flag {{.}}">{{.}}</div>
        {{/flags}}
        <span>{{#returns}}
            <span class="valsep">,</span>
            <span class="types">{{#valtype}}
                <span class="valsep punctuation">|</span>
                <a class="valtype" href="{{link path}}">{{name}}</a>{{#if isPointer}}<span class="star">*</span>{{/if}}
                {{#if generics}}
                    <span class="punctuation">&lt;</span>
                    <span>{{#generics}}
                        <a class="valtype" href="{{link path}}">{{name}}</a>
                        <span class="valsep">,</span>
                    {{/generics}}</span>
                    <span class="punctuation">&gt;</span>
                {{/if}}
            {{/valtype}}</span>
        {{/returns}}</span>
        <a href="{{link path}}" class="title">
            {{name}}
        </a>
        (
            <span>{{#arguments}}
{{> lightArgument}}
            {{/arguments}}</span>
        ) {
    </span>
    {{#if isInherited}}
        <div class="source super">
            Inherited from <a class="super" href="{{link source}}">{{sourcestr}}</a>
        </div>
    {{else}}
        {{#override}}
            <div class="source super">
                Overrides <a class="super" href="{{link path}}">{{name}}</a>
            </div>
        {{/override}}
    {{/if}}
    {{#satisfies}}
        <div class="source interface">
            Satisfies interface <a class="interface" href="{{link path}}">{{pathstr}}</a>
        </div>
    {{/satisfies}}
    <div class="info">
        <span class="markdown">{{{markdown summaryDoc}}}</span>
        {{#if signatures}}
            {{#signatures}}
{{> signature}}
            {{/signatures}}
        {{/if}}
        {{#if arguments}}
            <h4>Arguments</h4>
            <div class="section arguments">
                {{#arguments}}
                    <div class="child argument {{#if isCallback}}callback{{/if}}" id="{{elemID}}">
{{> argumentSignature}}
                        <div class="info">
                            <span class="markdown">{{{markdown summaryDoc}}}</span>
                            {{#if isCallback}}
                                {{#if arguments}}
                                    <h4>Arguments</h4>
                                    <div class="section arguments">
                                        {{#arguments}}
                                            <div class="child {{ctype}}" id="{{elemID}}">
                                                <span class="signature">
                                                    {{#if isKwarg}}
                                                        <a class="title" href="{{link path}}">
                                                            {{name}}
                                                        </a>
                                                        =
                                                        <span>{{#valtype}}
                                                            <span class="valsep">|</span>
                                                            <a class="valtype" href="{{link path}}">{{name}}</a>
                                                        {{/valtype}}</span>
                                                    {{else}}
                                                        <span>{{#valtype}}
                                                            <span class="valsep">|</span>
                                                            <a class="valtype" href="{{link path}}">{{name}}</a>
                                                        {{/valtype}}</span>
                                                        <a class="title" href="{{link path}}">
                                                            {{name}}
                                                        </a>
                                                    {{/if}}
                                                    {{#if isInherited}}
                                                        <div class="source">
                                                            Inherited from <a class="super" href="{{link source}}">{{sourcestr}}</a>
                                                        </div>
                                                    {{/if}}
                                                </span>
                                                <div class="info">
                                                    <span class="markdown">{{{markdown summaryDoc}}}</span>
                                                </div>
                                            </div>
                                        {{/arguments}}
                                    </div>
                                {{/if}}
                                {{#args}}
                                    <span class="signature">
                                        , ...
                                        <span>{{#valtype}}
                                            <span class="valsep">|</span>
                                            <a class="valtype" href="{{link path}}">{{name}}</a>
                                        {{/valtype}}</span>
                                        <a class="title" href="{{link path}}">
                                            {{name}}
                                        </a>
                                    </span>
                                {{/args}}
                                {{#kwargs}}
                                    <span class="signature">
                                        , ...
                                        <span>{{#valtype}}
                                            <span class="valsep">|</span>
                                            <a class="valtype" href="{{link path}}">{{name}}</a>
                                        {{/valtype}}</span>
                                        **<a class="title" href="{{link path}}">
                                            {{name}}
                                        </a>
                                    </span>
                                {{/kwargs}}
                                {{#if returns}}
                                <h5>Returns</h5>
                                <div class="section returns">
                                    {{#returns}}
                                        <div class="return{{#if isSymbol}} symbol{{/if}}">
{{#if isSymbol}}
{{> symbolChild}}
{{else}}
{{> child}}
{{/if}}
                                        </div>
                                    {{/returns}}
                                </div>
                                {{/if}}
                            {{/if}}
                        </div>
                    </div>
                {{/arguments}}
            </div>
        {{/if}}
        {{#if returns}}
            <h4>Returns</h4>
            <div class="section returns">
                {{#returns}}
                <div class="return{{#if isSymbol}} symbol{{/if}}">
{{#if isSymbol}}
{{> symbolChild}}
{{else}}
{{> child}}
{{/if}}
                </div>
                {{/returns}}
            </div>
        {{/if}}
        <div class="tail" /></div>
    </div>
</div>
